    <link rel="stylesheet" href="http://s3.amazonaws.com/codecademy-content/projects/bootstrap.min.css">
    <link href='http://fonts.googleapis.com/css?family=Roboto:100,300,400,700,500' rel='stylesheet' type='text/css'><div class="row">
  
  <% @albums.each do |album| %> 
  <div class="col-md-3">
    <%= link_to(album) do %>
      <div class="thumbnail">
        <%= image_tag(album.cover) %>
        <div class="caption">
          <h3 class="title"><%= album.title %></h3>
          <p class="artist"><%= album.artist %></p>
        </div>
      </div>
    <% end %>
  </div>
  <% end %>

  
</div>



